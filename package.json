{
	"name": "lime-viewer",
	"version": "1.0.0",
	"description": "A browser-based viewer for LINE backup data exported from LIMEs",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/Nich87/LIME-Viewer.git"
	},
	"homepage": "https://github.com/Nich87/LIME-Viewer",
	"bugs": {
		"url": "https://github.com/Nich87/LIME-Viewer/issues"
	},
	"author": "Nich87",
	"license": "MIT",
	"keywords": [
		"line",
		"lime",
		"backup",
		"viewer",
		"svelte",
		"sveltekit"
	],
	"lint-staged": {
		"*.{js,ts,svelte}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,md,css,html}": [
			"prettier --write"
		]
	},
	"scripts": {
		"dev": "vite dev",
		"dev:host": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview --host",
		"prepare": "husky",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@commitlint/cli": "^20.2.0",
		"@commitlint/config-conventional": "^20.2.0",
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.2",
		"@libsql/client": "^0.15.15",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^22.19.3",
		"@vite-pwa/sveltekit": "^1.1.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"husky": "^9.1.7",
		"lint-staged": "^16.2.7",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.46.1",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.51.0",
		"vite": "^7.3.0",
		"vite-plugin-pwa": "^1.2.0",
		"workbox-window": "^7.4.0"
	},
	"dependencies": {
		"@iconify/svelte": "^5.1.0",
		"html-to-image": "^1.11.13",
		"jszip": "^3.10.1",
		"sql.js": "^1.13.0"
	}
}
